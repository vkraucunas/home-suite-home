[lints]
all=error


[include]
src/*

[ignore]
.*/node_modules/.*


[libs]
flow-typed

[options]
all=true
emoji=true
module.use_strict=true
module.file_ext=.mjs
module.file_ext=.js
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

suppress_type=$FlowShutUp
suppress_type=$FixThis
suppress_type=$ExpectedFlowError

suppress_comment=\\$FlowShutUp
suppress_comment=\\$FixThis
suppress_comment=\\$ExpectedFlowError
